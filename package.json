{
  "name": "kafedra",
  "version": "1.0.1",
  "description": "Underground psychedelic russian rock group site",
  "main": "index.js",
  "scripts": {
    "buildDev": "rm -rf build && webpack --mode development --config webpack/webpack.server.config.js && webpack --mode development --config webpack/webpack.dev.config.js",
    "buildProd": "rm -rf build && webpack --mode production --config webpack/webpack.server.config.js && webpack --mode production --config webpack/webpack.prod.config.js",
    "start": "node ./build/server.js",
    "build:songs:json": "node src/server/buildSongsJson.js",
    "eslint": "eslint ./webpack ./src",
    "stylelint": "stylelint ./src/styles",
    "build:stats": "npm run buildDev && webpack --mode development --config=webpack/webpack.dev.config.js --profile --json > stats.json",
    "view:stats": "webpack-bundle-analyzer stats.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "fs-extra": "^8.1.0",
    "howler": "^2.1.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "nunjucks-webpack-plugin": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "parallax-js": "^3.1.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.35.0",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "engines": {
    "node": ">=4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "json-loader": "^0.5.7",
    "node-sass": "^4.12.0",
    "nunjucks": "^3.2.0",
    "postcss-loader": "^3.0.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "stylelint": "^10.1.0",
    "url-loader": "^1.1.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2"
  }
}
